{
  "name": "electron-with-nextjs",
  "description": "Demonstrates spawning a Next.js application from Electron and using it to serve content",
  "title": "Electron With Next.JS",
  "version": "0.1.0",
  "main": "electron-main.js",
  "authors": [
    "Frank Hale <frankhale@gmail.com>"
  ],
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": ".\\node.exe start-electron.js"
  },
  "app": {
    "react-devtools-path": "react-devtools",
    "url": "http://127.0.0.1",
    "port": 62000,
    "ioPort": 62001,
    "node": {
      "exe": ".\\node.exe",
      "args": [
        "server.js"
      ],
      "production": false
    }
  },
  "dependencies": {
    "electron": "1.7.1",
    "express": "^4.15.3",
    "jquery": "^3.2.1",
    "keymaster": "^1.6.2",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.0",
    "next": "^2.4.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-tap-event-plugin": "^2.0.1",
    "request": "^2.81.0",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1",
    "strip": "^3.0.0"
  }
}